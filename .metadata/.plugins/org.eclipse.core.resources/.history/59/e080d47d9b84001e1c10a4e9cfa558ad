package com.shoplaptop.entity;

import java.util.Date;

public class PhieuDoi {
	private int ID;
	private String maPhieuDoi;
	private String MaKH ;
	private String MaHD ;
	private  Date NgayTao;
	private String liDo;
	private int SerialOld ;
	private int SerialNew;
	public PhieuDoi() {
		
	}
	public PhieuDoi(int iD, String maPhieuDoi, String maKH, String maHD, Date ngayTao, String liDo, int serialOld,
			int serialNew) {
		
		ID = iD;
		this.maPhieuDoi = maPhieuDoi;
		MaKH = maKH;
		MaHD = maHD;
		NgayTao = ngayTao;
		this.liDo = liDo;
		SerialOld = serialOld;
		SerialNew = serialNew;
	}
	
	
	public PhieuDoi(String maPhieuDoi, String maKH, String maHD, Date ngayTao, String liDo) {
		this.maPhieuDoi = maPhieuDoi;
		MaKH = maKH;
		MaHD = maHD;
		NgayTao = ngayTao;
		this.liDo = liDo;
	}
	
	
	public PhieuDoi(int iD, int serialOld, int serialNew) {
		
		ID = iD;
		SerialOld = serialOld;
		SerialNew = serialNew;
	}
	public int getID() {
		return ID;
	}
	public void setID(int iD) {
		ID = iD;
	}
	public String getMaPhieuDoi() {
		return maPhieuDoi;
	}
	public void setMaPhieuDoi(String maPhieuDoi) {
		this.maPhieuDoi = maPhieuDoi;
	}
	public String getMaKH() {
		return MaKH;
	}
	public void setMaKH(String maKH) {
		MaKH = maKH;
	}
	public String getMaHD() {
		return MaHD;
	}
	public void setMaHD(String maHD) {
		MaHD = maHD;
	}
	public Date getNgayTao() {
		return NgayTao;
	}
	public void setNgayTao(Date ngayTao) {
		NgayTao = ngayTao;
	}
	public String getLiDo() {
		return liDo;
	}
	public void setLiDo(String liDo) {
		this.liDo = liDo;
	}
	public int getSerialOld() {
		return SerialOld;
	}
	public void setSerialOld(int serialOld) {
		SerialOld = serialOld;
	}
	public int getSerialNew() {
		return SerialNew;
	}
	public void setSerialNew(int serialNew) {
		SerialNew = serialNew;
	}
	@Override
	public String toString() {
		return "PhieuDoi [ID=" + ID + ", maPhieuDoi=" + maPhieuDoi + ", MaKH=" + MaKH + ", MaHD=" + MaHD + ", NgayTao="
				+ NgayTao + ", liDo=" + liDo + ", SerialOld=" + SerialOld + ", SerialNew=" + SerialNew + "]";
	}
	
	
	
	
}
