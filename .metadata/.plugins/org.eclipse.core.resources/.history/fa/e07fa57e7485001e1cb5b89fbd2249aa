package com.shoplaptop.entity;

import java.lang.invoke.StringConcatFactory;

public class CTHoaDon {
	private int ID_HoaDon ;
	private String MaHD;
	private int ID_Serial;
	private String SerialNumber;
	
	public CTHoaDon() {
		// TODO Auto-generated constructor stub
	}

	public CTHoaDon(int iD_HoaDon, String maHD, int iD_Serial, String serialNumber) {
		super();
		ID_HoaDon = iD_HoaDon;
		MaHD = maHD;
		ID_Serial = iD_Serial;
		SerialNumber = serialNumber;
	}

	public int getID_HoaDon() {
		return ID_HoaDon;
	}

	public void setID_HoaDon(int iD_HoaDon) {
		ID_HoaDon = iD_HoaDon;
	}

	public String getMaHD() {
		return MaHD;
	}

	public void setMaHD(String maHD) {
		MaHD = maHD;
	}

	public int getID_Serial() {
		return ID_Serial;
	}

	public void setID_Serial(int iD_Serial) {
		ID_Serial = iD_Serial;
	}

	public String getSerialNumber() {
		return SerialNumber;
	}

	public void setSerialNumber(String serialNumber) {
		SerialNumber = serialNumber;
	}

	@Override
	public String toString() {
		return "CTHoaDon [ID_HoaDon=" + ID_HoaDon + ", MaHD=" + MaHD + ", ID_Serial=" + ID_Serial + ", SerialNumber="
				+ SerialNumber + "]";
	}
	
	
}
